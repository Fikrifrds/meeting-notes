{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:app:default",
    "core:event:default",
    "core:window:default",
    "opener:default",
    {
      "identifier": "core:app:allow-app-hide",
      "allow": []
    },
    {
      "identifier": "core:app:allow-app-show",
      "allow": []
    }
  ]
}
